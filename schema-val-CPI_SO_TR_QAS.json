{
  "$schema": "http://json-schema.org/draft-08/schema#",
  "type": "object",
  "properties": {
    "hash": { "type": "string" },
    "entity": { "type": "string" },
    "orderId": { "type": "string" },
    "restaurantId": { "type": "string" },
    "salesForceGridId": { "type": "string" },
    "vertical": { "type": "string" },
    "currency": { "type": "string" },
    "domain": { "type": "string" },
    "orderCity": { "type": "string" },
    "legalName": { "type": "string" },
    "vendorName": { "type": "string" },
    "chainName": { "type": "string" },
    "chainCode": { "type": "string" },
    "chainURLKey": { "type": "string" },
    "status": { "type": "string" },
    "deliveryServiceProvider": { "type": "string" },
    "orderDate": { "type": "string"},
    "orderTime": { "type": "string" },
    "timestamp": { "type": "string" },
    "deliveryDate": { "type": "string"},
    "expectedDeliveryDate": { "type": "string" },
    "paymentData": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "amount": { "type": "string" },
          "paymentMethod": { "type": "string" },
          "paymentProvider": { "type": "string" },
          "paymentServiceProvider": { "type": "string" },
          "acquirerIdByBank": { "type": "string" }
        },
        "required": ["amount", "paymentMethod", "paymentProvider"]
      }
    },
    "isCorporate": { "type": "boolean" },
    "isProSubscription": { "type": "boolean" },
    "commissionBaseModel": { "type": "string" },
    "rejectionReason": { "type": "integer" },
    "items": {
      "type": "object",
      "properties": {
        "condition": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "conditionType": { "type": "string" },
              "conditionValue": { "type": "string" }
            },
            "required": ["conditionType", "conditionValue"]
          }
        }
      },
      "required": ["condition"]
    }
  },
  "required": [
    "hash",
    "entity",
    "orderId",
    "restaurantId",
    "salesForceGridId",
    "vertical",
    "currency",
    "domain",
    "orderCity",
    "legalName",
    "vendorName",
    "status",
    "deliveryServiceProvider",
    "orderDate",
    "orderTime",
    "timestamp",
    "deliveryDate",
    "expectedDeliveryDate",
    "paymentData",
    "isCorporate",
    "isProSubscription",
    "commissionBaseModel",
    "rejectionReason",
    "items"
  ]
}
